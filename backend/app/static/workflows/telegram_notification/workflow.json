{
  "name": "Telegram Notification Sender",
  "nodes": [
    {
      "parameters": {},
      "id": "fb8bb41b-6bc3-44e3-82f6-eba5e6b67c53",
      "name": "Start",
      "type": "n8n-nodes-base.start",
      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "chatId": "={{ $json.chat_id }}",
        "text": "={{ $json.message }}",
